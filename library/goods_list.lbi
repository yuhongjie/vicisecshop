                                        }
                                    }
                                    function setSorting() {
                                        var strSorting = getUrlParam('sort');
                                        if(strSorting=='goods_id'){
                                             window.location.href="{$script_name}.php?category={$category}&display={$pager.display}&brand={$brand_id}&price_min={$price_min}&price_max={$price_max}&filter_attr={$filter_attr}&page={$pager.page}&sort=goods_id&order=<!-- {if $pager.sort == 'goods_id' && $pager.order == 'DESC'} -->ASC<!-- {else} -->DESC<!-- {/if} -->&size={$pager.size}#goods_list"; 
